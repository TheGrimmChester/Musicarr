{
    "name": "thegrimmchester/musicarr",
    "description": "A complete music library management solution developed with Symfony and the MusicBrainz API.",
    "type": "project",
    "license": "EUPL-1.2",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.5",
        "doctrine/cache": "^2.2",
        "doctrine/doctrine-bundle": "^2.15.1",
        "doctrine/doctrine-migrations-bundle": "^3.4.2",
        "doctrine/orm": ">=3.5.2",
        "guzzlehttp/guzzle": "^7.9.3",
        "james-heinrich/getid3": "^1.9.23",
        "predis/predis": "^3.2",
        "sylius/twig-hooks": "^0.8.1",
        "symfony/apache-pack": ">=1.0.1",
        "symfony/cache": "7.3.*",
        "symfony/config": "7.3.*",
        "symfony/console": "7.3.*",
        "symfony/doctrine-bridge": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/flex": "^2.8.1",
        "symfony/form": "7.3.*",
        "symfony/framework-bundle": "7.3.*",
        "symfony/http-client": "7.3.*",
        "symfony/lock": "7.3.*",
        "symfony/maker-bundle": "^1.64",
        "symfony/monolog-bundle": "^3.10",
        "symfony/runtime": "7.3.*",
        "symfony/security-bundle": "7.3.*",
        "symfony/stimulus-bundle": "^2.29.2",
        "symfony/translation": "7.3.*",
        "symfony/twig-bundle": "7.3.*",
        "symfony/ux-live-component": ">=2.29.2",
        "symfony/ux-twig-component": "^2.29.2",
        "symfony/validator": "7.3.*",
        "symfony/webpack-encore-bundle": "^2.3",
        "symfony/yaml": "7.3.*",
        "twig/extra-bundle": "^3.21",
        "twig/intl-extra": "^3.21",
        "twig/twig": "^3.21.1"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "phpro/grumphp": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "config/_plugins_autoload.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "docker-install": [
            "composer install --prefer-dist --no-interaction --no-progress --no-scripts",
            "composer run-script auto-scripts"
        ],
        "docker-build": [
            "composer install --prefer-dist --no-interaction --no-progress --no-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.86.0",
        "phpmd/phpmd": "^2.15",
        "phpro/grumphp": "^2.14",
        "phpstan/phpstan": "^1.12.28",
        "phpunit/phpunit": "^10.5.52",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/phpunit-bridge": "^7.3.1",
        "symfony/stopwatch": "7.3.*",
        "symfony/web-profiler-bundle": "7.3.*",
        "vincentlanglet/twig-cs-fixer": "^3.9"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        },
        "installer-paths": {
            "plugins/{$name}/": [
                "type:musicarr-plugin"
            ]
        }
    }
}
